    // Window
    "window.zoomLevel": 0,
    "window.nativeTabs": true,
    // Terminal
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.copyOnSelection": true,
    // Plugin
    // - Material Icon Theme
    //     "material-icon-theme.showUpdateMessage": false,
    // - ESLint
    "eslint.enable": true,
    "eslint.autoFixOnSave": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact"
    ],
    "eslint.run": "onSave",
    // // - Flow
    // "flow.useNPMPackagedFlow": true,
    // - Dart Code
    "dart.sdkPath": "~/repos/flutter/bin/cache/dark-sdk",
    "dart.debugSdkLibraries": true,
    "dart.debugExternalLibraries": true,
    "dart.showTodos": true,
    "dart.lineLength": 100,
    // // - Standard Style
    // "standard.semistandard": true,
    // "standard.usePackageJson": true,
    // - Material theme
    //     "materialTheme.accent": "Cyan",
    // - GitLens
    //     "gitlens.advanced.messages": {
    //         "suppressCommitHasNoPreviousCommitWarning": false,
    //         "suppressCommitNotFoundWarning": false,
    //         "suppressFileNotUnderSourceControlWarning": false,
    //         "suppressGitVersionWarning": false,
    //         "suppressLineUncommittedWarning": false,
    //         "suppressNoRepositoryWarning": false,
    //         "suppressResultsExplorerNotice": false,
    //         "suppressShowKeyBindingsNotice": true,
    //         "suppressUpdateNotice": true,
    //         "suppressWelcomeNotice": false
    //     },
    //     "gitlens.keymap": "alternate",
    // Editor
    //     "editor.stablePeek": true,
    "editor.renderIndentGuides": true,
    "editor.autoIndent": true,
    "editor.renderWhitespace": "all",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.tabSize": 2,
    // Javascript
    "javascript.validate.enable": false,
    "javascript.format.enable": false,
    "javascript.format.insertSpaceBeforeFunctionParenthesis": false,
    // JSON
    "json.format.enable": false,
    // Typescript
    //     "typescript.autoImportSuggestions.enabled": false,
    //     "typescript.format.insertSpaceBeforeFunctionParenthesis": false,
    //     "typescript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
    // File Associations
    "files.associations": {
        "*bash_profile*": "shellscript",
        "*bash_aliases": "shellscript",
        "*rc": "shellscript",
        "*Dockerfile*": "dockerfile",
        ".gitignore": "shellscript",
        ".bash_functions": "shellscript",
        ".bash_exports": "shellscript",
        "**/task-*.yml": "concourse-task-yaml"
    },
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "[concourse-pipeline-yaml]": {
    },
    "editor.codeLens": false,
    "gitlens.views.repositories.location": "gitlens",
    "gitlens.views.fileHistory.location": "gitlens",
    "gitlens.views.lineHistory.location": "gitlens",
    "gitlens.views.compare.location": "gitlens",
    "gitlens.views.search.location": "gitlens",
    "go.useLanguageServer": true,
    "atlascode.jira.workingSite": {
        "baseUrlSuffix": "atlassian.net"
    },
    "yaml.schemas": {
        "file:///Users/michael.perrotte/.vscode/extensions/atlassian.atlascode-1.4.3/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    },
    "atlascode.jira.workingProject": {
        "id": "11408",
        "name": "Build and Tooling",
        "key": "TOOL"
    },
    "yaml.customTags": [
        "!Ref scalar",
        "!Sub scalar",
        "!GetAtt scalar",
        "!Condition scalar",
        "!Join sequence",
        "!Equals sequence",
        "!And sequence",
        "!Not sequence",
        "!Or sequence"
    ],
    "atlascode.bitbucket.issues.monitorEnabled": false,
    "atlascode.bitbucket.pipelines.explorerEnabled": false,
    "atlascode.bitbucket.pipelines.monitorEnabled": false
}
