{
  "name": "@mikemimik/dotfiles",
  "description": "Dotfiles CLI Tool",
  "version": "0.1.6",
  "author": "Michael Perrotte @mikemimik",
  "repository": "mikemimik/dotfiles",
  "license": "MIT",
  "main": "dist/index.js",
  "bin": {
    "dotfiles": "./bin/run"
  },
  "homepage": "https://github.com/mikemimik/dotfiles",
  "bugs": "https://github.com/mikemimik/dotfiles/issues",
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "oclif"
  ],
  "files": [
    "/bin",
    "/dist",
    "/npm-shrimkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -b",
    "format": "prettier --write .",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "postpack": "rm oclif.manifest.json",
    "test": "npm run build && npm link",
    "version": "oclif readme && git add README.md",
    "postversion": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@npmcli/git": "^3.0.1",
    "@npmcli/promise-spawn": "^3.0.0",
    "@oclif/core": "^1.13.10",
    "@oclif/plugin-help": "^5.1.12",
    "@oclif/plugin-version": "^1.1.1",
    "chalk": "^5.0.1",
    "debug": "^4.3.4",
    "dedent": "^0.7.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@oclif/test": "^2.1.1",
    "@tsconfig/node16": "^1.0.3",
    "@types/debug": "^4.1.7",
    "@types/dedent": "^0.7.0",
    "@types/node": "^18.7.2",
    "oclif": "^3.1.2",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "oclif": {
    "bin": "dotfiles",
    "dirname": "dotfiles",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-version"
    ],
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ],
    "topicSeparator": " ",
    "topics": {}
  }
}
